{
  "jira": {
    "linkBase": "https://{COMPANY}.atlassian.net",
    "projects": ["TEST"],
    "priorities": ["Highest", "High", "Medium", "Low", "Lowest"],
    "apiToken": null,
    "developmentStates": ["In Development"],
    "reviewStates": ["In Review"],
    "doneStates": ["Done", "Merged"],
    "query": "resolution = unresolved and status NOT IN ('In Review', 'Review', 'Merged', 'Archive') and assignee=currentuser()"
  },
  "github": {
    "repoName": "some-github/repo",
    "reviewers": ["cgsdev0"],
    "apiToken": null
  },
  "language": {
    "newTicket": "-> Create new ticket...",
    "newBranch": "-> Create new branch..."
  },
  "files": {
    "associations": "~/.jira-branch-associations",
    "daemonLogs": "~/.jira_daemon.log"
  },
  "configVersion": 1
}
